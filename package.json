{
  "name": "php-server",
  "main": "./lib/php-server",
  "version": "0.7.0",
  "description": "Run a local PHP server from within Atom, displaying the log in a bottom panel",
  "activationCommands": {
    "atom-workspace": "php-server:start",
    ".tree-view": [
      "php-server:start-tree",
      "php-server:start-tree-route"
    ],
    "atom-text-editor": "php-server:start-document"
  },
  "repository": "https://github.com/leijou/php-server",
  "license": "MIT",
  "engines": {
    "atom": ">0.50.0"
  },
  "dependencies": {
    "portfinder": "latest",
    "open": "latest",
    "atom-message-panel": "latest",
    "atom-space-pen-views": "^2.0.3"
  }
}
